{
  "data": {
    "personType": {
      "manager": {
        "name": "Justin"
      }
    }
  },
  "http-request": {
    "method": "GET",
    "path": "/person",
    "query": {
      "type": "(PERSON_TYPE:string)"
    }
  },
  "http-response": {
    "status": 200,
    "body": {
      "name": "$(data.personType[PERSON_TYPE].name",
      "type": "manager"
    }
  }
}