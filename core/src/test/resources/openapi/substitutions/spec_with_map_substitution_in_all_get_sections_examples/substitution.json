{
  "data": {
    "token": {
      "AB": {
        "region": "IN"
      }
    },
    "location": {
      "IN": {
        "currency": "INR"
      }
    },
    "id": {
      "abc": {
        "city": "Mumbai"
      }
    }
  },
  "http-request": {
    "method": "GET",
    "path": "/data/(ID:string)?location=(LOCATION:string)",
    "headers": {
      "X-Routing-Token": "(TOKEN:string)"
    }
  },
  "http-response": {
    "status": 200,
    "headers": {
      "X-Region": "$(data.token[TOKEN].region)"
    },
    "body": {
      "city": "$(data.id[ID].city)",
      "currency": "$(data.location[LOCATION].currency)"
    }
  }
}